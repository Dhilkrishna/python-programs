import pandas as pd

df = pd.read_csv("car_prices_3.csv")
print(df.head())      # first rows
df.info()      # column types and nulls
print(df.describe())  # summary stats for numeric columns
df.dropna() 

df.drop_duplicates(inplace=True)

print(df['odometer'].unique())
print(df['sellingprice'].value_counts())

print(df.groupby('state')['sellingprice'].mean())

best_product = df.groupby('COMPANY')['sellingprice'].sum().idxmax()
print(best_product)

df.groupby('state')['sellingprice'].sum().sort_values().plot(kind='bar', figsize=(12,6))

